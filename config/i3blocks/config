[arch-update]
command=~/.i3blocklets/arch-update/arch-update 
interval=3600
markup=pango

[volume]
command=~/.i3blocklets/volume/volume
label= 
interval=once
signal=10

[wifi]
command=~/.i3blocklets/wifi/wifi
INTERFACE=wlp3s0
interval=60
markup=pango
label= 

[iface]
command=~/.i3blocklets/iface/iface
color=#00FF00
interval=10

[battery2]
command=~/.i3blocklets/battery2/battery2
markup=pango
interval=30

[time]
command=date
interval=1

[key_layout]
command=setxkbmap -print | awk -F"+" '/xkb_symbols/ {print $2}'
interval=1
